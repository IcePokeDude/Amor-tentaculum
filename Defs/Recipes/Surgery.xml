    <anesthetize>false</anesthetize>
    <successfullyRemovedHediffMessage>{0} has successfully extracted the egg from {1}</successfullyRemovedHediffMessage>
    <skillRequirements>
      <Medicine>1</Medicine>
    </skillRequirements>
    <products>
      <BigTEgg>1</BigTEgg>
    </products>
  </RecipeDef>
