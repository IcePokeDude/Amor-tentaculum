      <HediffDef ParentName="ImplantHediffBase">
    <defName>Bworm</defName>
    <label>Breeding Worm</label>
    <labelNoun>a Breeding worm</labelNoun>
    <description>A parisitic worm attached to the brain stem. Keeps the victim aroused, energtic, and agressive.</description>
    <descriptionHyperlinks><ThingDef>BreedWorm</ThingDef></descriptionHyperlinks>
    <stages>
      <li>
        <hungerRateFactorOffset>0.15</hungerRateFactorOffset>
          <capMods>
            <li>
              <capacity>Consciousness</capacity>
              <offset>0.25</offset>
            </li>
            <li>
              <capacity>Moving</capacity>
              <offset>0.25</offset>
            </li>
          </capMods>
          <statOffsets>
          <SexFrequency>2</SexFrequency>
          <WorkSpeedGlobal>0.3</WorkSpeedGlobal>
        </statOffsets>
        <statFactors>
          <MeleeDamageFactor>1.25</MeleeDamageFactor>
        </statFactors>
      </li>
    </stages>
    <spawnThingOnRemoved>BreedWorm</spawnThingOnRemoved>
  </HediffDef>

  <ThingDef ParentName="BodyPartProstheticBase">
    <defName>FeedWorm</defName>
    <label>Feeding Tentacle</label>
    <description>A tentacle ment for insertion in the throat. Rapidly digests food for the host and injects it as a slurry down the throat. Makes the target immune to gutworms, and food poisoning.</description>
    <descriptionHyperlinks><RecipeDef>InstallfeedWorm</RecipeDef></descriptionHyperlinks>
    <thingSetMakerTags>
      <li>RewardStandardLowFreq</li>
    </thingSetMakerTags>
        <recipeMaker>
            <researchPrerequisite>ATWorm</researchPrerequisite>
            <recipeUsers>
              <li>DrugLab</li>
            </recipeUsers>
            <soundWorking>Recipe_Drug</soundWorking>
        </recipeMaker>
    <costList>
      <softresin>30</softresin>
      <MedTEgg>1</MedTEgg>
    </costList>
    <techHediffsTags>
      <li>Advanced</li>
    </techHediffsTags>
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallImplantBase">
    <defName>InstallfeedWorm</defName>
    <label>Insert feeding tentacle</label>
    <description>Insert a feeding tentacle.</description>
    <descriptionHyperlinks>
      <ThingDef>FeedWorm</ThingDef>
      <HediffDef>Fworm</HediffDef>
    </descriptionHyperlinks>
    <jobString>Inserting feeding tentacle.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>FeedWorm</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>FeedWorm</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Jaw</li>
    </appliedOnFixedBodyParts>
    <addsHediff>Fworm</addsHediff>
  </RecipeDef>

  <RecipeDef ParentName="SurgeryRemoveImplantBase">
    <defName>RemoveFeedworm</defName>
    <label>remove feeding tentacle</label>
    <description>Remove the feeding tentacle.</description>
    <descriptionHyperlinks>
      <ThingDef>FeedWorm</ThingDef>
      <HediffDef>Fworm</HediffDef>
    </descriptionHyperlinks>
    <jobString>Removing feeding tentacle.</jobString>
    <removesHediff>Fworm</removesHediff>
