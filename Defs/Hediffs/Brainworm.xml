<?xml version="1.0" encoding="utf-8"?>
<Defs>
      <HediffDef ParentName="ImplantHediffBase">
    <defName>IBworm</defName>
    <label>Indoctrination Worm</label>
    <labelNoun>a indoctrination worm</labelNoun>
    <description>A parisitic worm attached to the brain stem. Keeps the victim content and ready to die even under horrible conditions.</description>
    <descriptionHyperlinks><ThingDef>IndoWorm</ThingDef></descriptionHyperlinks>
        <disablesNeeds>
      <li>Beauty</li>
      <li>Outdoors</li>
        </disablesNeeds>
    <stages>
      <li>
        <blocksMentalBreaks>true</blocksMentalBreaks>
        <blocksInspirations>true</blocksInspirations>
        <pctAllThoughtNullification>1</pctAllThoughtNullification>
        <overrideMoodBase>0.9</overrideMoodBase>
        <partEfficiencyOffset>-0.20</partEfficiencyOffset>
        <painFactor>0</painFactor>
        <hungerRateFactorOffset>-0.15</hungerRateFactorOffset>
        <statOffsets>
          <Vulnerability>0.5</Vulnerability>
        </statOffsets>
        <statFactors>
          <RestFallRateFactor>0.5</RestFallRateFactor>
        </statFactors>
      </li>
    </stages>
    <spawnThingOnRemoved>IndoWorm</spawnThingOnRemoved>
  </HediffDef>

  <ThingDef ParentName="BodyPartProstheticBase">
    <defName>IndoWorm</defName>
    <label>Indoctrination Worm</label>
    <description>A parisitic worm to be implanted in the brain stem. Keeps the victim content and ready to die even under horrible conditions.</description>
    <descriptionHyperlinks><RecipeDef>InstallIndoWorm</RecipeDef></descriptionHyperlinks>
   <graphicData>
      <texPath>Things/Items/BrainLarve/WLarvae</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <thingSetMakerTags>
      <li>RewardStandardLowFreq</li>
    </thingSetMakerTags>
        <recipeMaker>
            <researchPrerequisite>ATWorm</researchPrerequisite>
            <recipeUsers>
              <li>DrugLab</li>
            </recipeUsers>
            <soundWorking>Recipe_Drug</soundWorking>
        </recipeMaker>
    <costList>
      <softresin>30</softresin>
      <MedTEgg>1</MedTEgg>
    </costList>
    <techHediffsTags>
      <li>Advanced</li>
    </techHediffsTags>
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallImplantBase">
    <defName>InstallIndoWorm</defName>
    <label>implant indoctrination worm</label>
    <description>Implant a indoctrination worm.</description>
    <descriptionHyperlinks>
      <ThingDef>IndoWorm</ThingDef>
      <HediffDef>IBworm</HediffDef>
    </descriptionHyperlinks>
    <jobString>Implanting indoctrination worm.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>IndoWorm</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>IndoWorm</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Brain</li>
    </appliedOnFixedBodyParts>
    <addsHediff>IBworm</addsHediff>
  </RecipeDef>

  <RecipeDef ParentName="SurgeryRemoveImplantBase">
    <defName>RemoveIBworm</defName>
    <label>remove indoctrination worm</label>
    <description>Remove the indoctrination worm.</description>
    <descriptionHyperlinks>
      <ThingDef>IndoWorm</ThingDef>
      <HediffDef>IBworm</HediffDef>
    </descriptionHyperlinks>
    <jobString>Removing indoctrination worm.</jobString>
    <removesHediff>IBworm</removesHediff>
  </RecipeDef>

