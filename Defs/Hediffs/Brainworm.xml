            <soundWorking>Recipe_Drug</soundWorking>
        </recipeMaker>
    <costList>
      <softresin>30</softresin>
      <MedTEgg>1</MedTEgg>
    </costList>
    <techHediffsTags>
      <li>Advanced</li>
    </techHediffsTags>
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallImplantBase">
