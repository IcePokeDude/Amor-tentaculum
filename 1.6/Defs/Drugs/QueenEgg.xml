<Defs>
    <ThingDef ParentName="BodyPartProstheticBase">
        <defName>QueenEgg</defName>
        <label>Amor Queen Egg</label>
        <description>An Amor Tentaculum queen egg. When eatten will hatch in the hosts womb. Leathal for pawns without a vagina.</description>
        <graphicData>
            <texPath>Things/Items/StartEgg</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
            <color>(256,256,256)</color>
        </graphicData>
        <statBases>
            <WorkToMake>500</WorkToMake>
            <MarketValue>600</MarketValue>
            <Mass>0.04</Mass>
        </statBases>
        <recipeMaker>
            <researchPrerequisite>AmorTentaculumCultavation</researchPrerequisite>
            <recipeUsers>
              <li>DrugLab</li>
            </recipeUsers>
            <soundWorking>Recipe_Drug</soundWorking>
        </recipeMaker>
    <techHediffsTags>
      <li>Advanced</li>
    </techHediffsTags>
    <costList>
      <HumpShroom>5</HumpShroom>
      <Neutroamine>30</Neutroamine>
      <Luciferium>3</Luciferium>
    </costList>
    </ThingDef>

  <RecipeDef ParentName="SurgeryFlesh">
    <defName>InstallAmorQueen</defName>
    <label>implant amor queen egg</label>
    <description>Implant an amor queen. Lethal for pawns without a vagina.</description>
    <descriptionHyperlinks>
      <ThingDef>QueenEgg</ThingDef>
      <HediffDef>queenimplant</HediffDef>
    </descriptionHyperlinks>
    <jobString>Implanting queen egg.</jobString>
    <workerClass>Recipe_InstallImplant</workerClass>
    <anesthetize>false</anesthetize>
    <surgerySuccessChanceFactor>100</surgerySuccessChanceFactor>
    <workAmount>500</workAmount>
    <developmentalStageFilter>Adult</developmentalStageFilter>
    <skillRequirements>
      <Medicine>0</Medicine>
    </skillRequirements>
    <recipeUsers>
      <li>Human</li>
      <li MayRequire="Ludeon.RimWorld.Anomaly">CreepJoiner</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>QueenEgg</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>QueenEgg</li>
      </thingDefs>
    </fixedIngredientFilter>
        <appliedOnFixedBodyParts>
      <li>Genitals</li>
    </appliedOnFixedBodyParts>
    <addsHediff>queenimplant</addsHediff>
  </RecipeDef>

