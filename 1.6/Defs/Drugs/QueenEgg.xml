<HediffDef>
    <defName>dickchech</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>host rejection</label>
    <labelNoun>rejection</labelNoun>
    <description>A queen tentacle egg is rejecting this host body. It's a slow and painful death.</description>
    <defaultLabelColor>(2,2,2)</defaultLabelColor>
    <minSeverity>0</minSeverity>
    <initialSeverity>0</initialSeverity>
    <maxSeverity>1</maxSeverity>
    <stages>
      <li>
        <minSeverity>0.001</minSeverity>
        <painOffset>2</painOffset>
        <statOffsets>
          <MoveSpeed>-0.5</MoveSpeed>
        </statOffsets>
        <hediffGivers>
          <li Class="HediffGiver_Random">
            <hediff>OrganDecay</hediff>
            <mtbDays>0.6</mtbDays>
            <partsToAffect>
              <li>Stomach</li>
              <li>Anus</li>
              <li>Genitals</li>
              <li>Neck</li>
              <li>Jaw</li>
              <li>Heart</li>
              <li>Liver</li>
              <li>Brain</li>
            </partsToAffect>
            <countToAffect>1</countToAffect>
            </li>
        </hediffGivers>
      </li>
    </stages>
    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>-0.6</severityPerDay>
      </li>
            <li Class="EBSGFramework.HediffCompProperties_SeverityByOtherHediffSeverities">
                <baseSeverity>0</baseSeverity>
                <hediffSets>
                    <li>
                        <hediff>Penis</hediff>
                        <factor>1</factor>
                    </li>
                    <li>
                        <hediff>DragonPenis</hediff>
                        <factor>1</factor>
                    </li>
                    <li>
                        <hediff>DemonPenis</hediff>
                        <factor>1</factor>
                    </li>
                    <li>
                        <hediff>DogPenis</hediff>
                        <factor>1</factor>
                    </li>
                    <li>
                        <hediff>HorsePenis</hediff>
                        <factor>1</factor>
                    </li>
                    <li>
                        <hediff>RodentPenis</hediff>
                        <factor>1</factor>
                    </li>
                </hediffSets>
            </li>
                        <li Class="EBSGFramework.HediffCompProperties_GiveOtherHediffPeriodically">
                <hediff>queenimplant</hediff>
                <severity>-2</severity>
                <interval>20</interval>
                <bodyParts>
                  <li>Anus</li>
                </bodyParts>
                <validSeverities>0.1~0.9</validSeverities>
            </li>
    </comps>
  </HediffDef>

