        </texPaths>
        <parentTagDef>Body</parentTagDef>
        <overrideMeshSize>1</overrideMeshSize>
        <drawSize>2</drawSize>
        <colorType>Skin</colorType>
        <rotDrawMode>Fresh, Rotting</rotDrawMode>
        <useSkinShader>true</useSkinShader>
        <useRottenColor>true</useRottenColor>
        <rotationRange>-8~8</rotationRange>
        <durationTicksRange>5~20</durationTicksRange>
        <nextSpasmTicksRange>5~25</nextSpasmTicksRange>
        <drawData>
          <scaleOffsetByBodySize>true</scaleOffsetByBodySize>
          <defaultData>
            <layer>49</layer>
          </defaultData>
          <dataNorth>
            <rotationOffset>310</rotationOffset>
            <flip>true</flip>
            <layer>90</layer>
            <offset>(0, 0, -0.5)</offset>
          </dataNorth>
          <dataEast>
            <rotationOffset>190</rotationOffset>
            <flip>true</flip>
            <offset>(-0.25, 0, -0.3)</offset>
            <layer>0</layer>
          </dataEast>
          <dataSouth>
            <rotationOffset>270</rotationOffset>
            <offset>(-0.2, 0, -0.5)</offset>
            <layer>0</layer>
          </dataSouth>
          <dataWest>
            <rotationOffset>190</rotationOffset>
            <flip>false</flip>
            <offset>(0.25, 0, -0.3)</offset>
            <layer>0</layer>
          </dataWest>
        </drawData>
