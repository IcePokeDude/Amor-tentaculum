    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>0.25</severityPerDay>
      </li>
            <li Class="EBSGFramework.HediffCompProperties_GiveOtherHediffPeriodically">
                <hediff>VStorage</hediff>
                <severity>3</severity>
                <interval>5000</interval>
                <bodyParts>
                  <li>Genitals</li>
                </bodyParts>
                <validSeverities>0.7~0.723</validSeverities>
            </li>
    </comps>
  </HediffDef>



<HediffDef>
    <defName>queenadult</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>Amor Pregnancy</label>
    <labelNoun>Pregnancy</labelNoun>
    <description>An Adult Amor Tentaculum queen growing within the host causing a permanent pregnancy. The host can now begin laying Amor eggs and emits soothing pheromones around them.</description>
    <defaultLabelColor>(203,120,227)</defaultLabelColor>
    <scenarioCanAdd>true</scenarioCanAdd>
    <maxSeverity>1.5</maxSeverity>
    <isBad>false</isBad>
    <tendable>false</tendable>
    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>0.01</severityPerDay>
      </li>
      <li Class="EBSGFramework.HediffCompProperties_CreateOtherHediffs">
          <hediffSets>
                <li>
                  <hediffDef>transform</hediffDef>
                  <validSeverity>1.5</validSeverity>
                  <severityPerDay>0</severityPerDay>
                  <initialSeverity>0.1</initialSeverity>
                </li>
          </hediffSets>
      </li>
        <li Class="HediffCompProperties_GiveHediffsInRange">
        <range>25</range>
        <hideMoteWhenNotDrafted>true</hideMoteWhenNotDrafted>
        <targetingParameters>
          <canTargetBuildings>false</canTargetBuildings>
          <canTargetAnimals>true</canTargetAnimals>
          <canTargetMechs>false</canTargetMechs>
          <onlyTargetColonists>true</onlyTargetColonists>
        </targetingParameters>
        <hediff>painsooth</hediff>
      </li>
    </comps>
    <stages>
      <li>
        <label>adult</label>
        <minSeverity>0.01</minSeverity>
        <restFallFactor>1.2</restFallFactor>
        <hungerRateFactorOffset>0.5</hungerRateFactorOffset>
        <regeneration>10</regeneration>
        <statOffsets>
          <PsychicSensitivity>0.4</PsychicSensitivity>
          <Vulnerability>0.5</Vulnerability>
          <SexFrequency>1</SexFrequency>
          <ArmorRating_Sharp>0.2</ArmorRating_Sharp>
          <ArmorRating_Blunt>0.5</ArmorRating_Blunt>
          <ArmorRating_Heat>0.7</ArmorRating_Heat>
          <ComfyTemperatureMin>-16</ComfyTemperatureMin>
        </statOffsets>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.01</offset>
          </li>
        </capMods>
      </li>
      <li>
        <label>maiden</label>
        <minSeverity>0.2</minSeverity>
        <restFallFactor>1.2</restFallFactor>
        <hungerRateFactorOffset>0.5</hungerRateFactorOffset>
        <regeneration>20</regeneration>
        <statOffsets>
          <PsychicSensitivity>0.5</PsychicSensitivity>
          <Vulnerability>0.5</Vulnerability>
          <SexFrequency>1.2</SexFrequency>
          <ArmorRating_Sharp>0.3</ArmorRating_Sharp>
          <ArmorRating_Blunt>0.6</ArmorRating_Blunt>
          <ArmorRating_Heat>0.8</ArmorRating_Heat>
          <ComfyTemperatureMin>-18</ComfyTemperatureMin>
        </statOffsets>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.03</offset>
          </li>
        </capMods>
      </li>
      <li>
        <label>empress</label>
        <minSeverity>0.6</minSeverity>
        <restFallFactor>1.2</restFallFactor>
        <hungerRateFactorOffset>1</hungerRateFactorOffset>
        <regeneration>30</regeneration>
        <statOffsets>
          <PsychicSensitivity>1</PsychicSensitivity>
          <Vulnerability>1</Vulnerability>
          <SexFrequency>2</SexFrequency>
          <ArmorRating_Sharp>0.4</ArmorRating_Sharp>
          <ArmorRating_Blunt>0.8</ArmorRating_Blunt>
          <ArmorRating_Heat>1</ArmorRating_Heat>
          <ComfyTemperatureMin>-20</ComfyTemperatureMin>
        </statOffsets>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.05</offset>
          </li>
        </capMods>
      </li>
      <li>
        <label>matriarch</label>
        <minSeverity>0.9</minSeverity>
        <restFallFactor>1.4</restFallFactor>
        <hungerRateFactorOffset>2</hungerRateFactorOffset>
        <regeneration>40</regeneration>
        <statOffsets>
          <PsychicSensitivity>2</PsychicSensitivity>
          <Vulnerability>1</Vulnerability>
          <SexFrequency>3</SexFrequency>
          <ArmorRating_Sharp>0.8</ArmorRating_Sharp>
          <ArmorRating_Blunt>1.6</ArmorRating_Blunt>
          <ArmorRating_Heat>2</ArmorRating_Heat>
          <ComfyTemperatureMin>-22</ComfyTemperatureMin>
        </statOffsets>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.1</offset>
          </li>
        </capMods>
      </li>
    </stages>
  </HediffDef>
</Defs>