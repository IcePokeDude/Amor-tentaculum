<?xml version="1.0" encoding="utf-8"?>
<Defs>
<HediffDef>
    <defName>queenimplant</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>Amor Pregnancy</label>
    <labelNoun>Pregnancy</labelNoun>
    <description>An Amor Tentaculum queen growing within the host. Unlike a normal pregnancy, the tentacle will not only grow but altar the host body to better suit tentacle breeding.</description>
    <defaultLabelColor>(203,120,227)</defaultLabelColor>
    <scenarioCanAdd>true</scenarioCanAdd>
    <maxSeverity>3</maxSeverity>
    <isBad>false</isBad>
    <tendable>false</tendable>
    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>0.07</severityPerDay>
      </li>
      <li Class="EBSGFramework.HediffCompProperties_CreateOtherHediffs">
          <hediffSets>
                <li>
                  <hediffDef>transform</hediffDef>
                  <validSeverity>0.98~1.01</validSeverity>
                  <severityPerDay>0</severityPerDay>
                  <initialSeverity>0.1</initialSeverity>
                </li>
                <li>
                  <hediffDef>queenadult</hediffDef>
                  <validSeverity>0.98~1.01</validSeverity>
                  <severityPerDay>0</severityPerDay>
                  <initialSeverity>0.1</initialSeverity>
                </li>
                <li>
                  <hediffDef>transformMale</hediffDef>
                  <validSeverity>2.8~3</validSeverity>
                  <severityPerDay>0</severityPerDay>
                  <initialSeverity>0.1</initialSeverity>
                </li>
          </hediffSets>
      </li>
        <li Class="EBSGFramework.HediffCompProperties_SeverityByOtherHediffSeverities">
                <baseSeverity>1</baseSeverity>
                <hediffSets>
                    <li>
                        <hediff>Penis</hediff>
                        <factor>1.5</factor>
                    </li>
                    <li>
                        <hediff>HorsePenis</hediff>
                        <factor>1.5</factor>
                    </li>
                    <li>
                        <hediff>DogPenis</hediff>
                        <factor>1.5</factor>
                    </li>
                    <li>
                        <hediff>DemonPenis</hediff>
                        <factor>1.5</factor>
                    </li>
                    <li>
                        <hediff>DragonPenis</hediff>
                        <factor>1.5</factor>
                    </li>
                </hediffSets>
            </li>
    </comps>
    <stages>
      <li>
        <label>incubating egg</label>
      </li>
      <li>
        <label>hatched</label>
        <minSeverity>0.1</minSeverity>
            <hungerRateFactorOffset>0.1</hungerRateFactorOffset>
        <statOffsets>
          <Vulnerability>0.1</Vulnerability>
          <MoveSpeed>-0.01</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <label>larvae</label>
        <minSeverity>0.15</minSeverity>
        <restFallFactor>1.05</restFallFactor>
        <hungerRateFactorOffset>0.15</hungerRateFactorOffset>
        <statOffsets>
          <PsychicSensitivity>0.1</PsychicSensitivity>
          <Vulnerability>0.2</Vulnerability>
          <MoveSpeed>-0.02</MoveSpeed>
        </statOffsets>
                <hediffGivers>
          <li Class="HediffGiver_Random">
            <hediff>tentaclegrowthS</hediff>
            <mtbDays>4</mtbDays>
            <partsToAffect>
              <li>Stomach</li>
            </partsToAffect>
            <countToAffect>1</countToAffect>
          </li>
        </hediffGivers>
        <painOffset>0.02</painOffset>
      </li>
      <li>
        <label>nymph</label>
        <minSeverity>0.4</minSeverity>
        <restFallFactor>1.1</restFallFactor>
        <hungerRateFactorOffset>0.3</hungerRateFactorOffset>
        <statOffsets>
          <PsychicSensitivity>0.2</PsychicSensitivity>
          <Vulnerability>0.3</Vulnerability>
          <SexFrequency>0.5</SexFrequency>
          <MoveSpeed>-0.05</MoveSpeed>
        </statOffsets>
        <hediffGivers>
          <li Class="HediffGiver_Random">
            <hediff>tentaclegrowthS</hediff>
            <mtbDays>3</mtbDays>
            <partsToAffect>
              <li>Stomach</li>
            </partsToAffect>
            <countToAffect>1</countToAffect>
          </li>
          <li Class="HediffGiver_Random">
            <hediff>tentaclegrowthA</hediff>
            <mtbDays>2</mtbDays>
            <partsToAffect>
              <li>Anus</li>
            </partsToAffect>
            <countToAffect>1</countToAffect>
          </li>
        </hediffGivers>
        <painOffset>0.03</painOffset>
      </li>
      <li>
        <label>nearly grown</label>
        <minSeverity>0.65</minSeverity>
        <restFallFactor>1.2</restFallFactor>
        <hungerRateFactorOffset>0.5</hungerRateFactorOffset>
        <statOffsets>
          <PsychicSensitivity>0.3</PsychicSensitivity>
          <Vulnerability>0.5</Vulnerability>
          <MoveSpeed>-0.1</MoveSpeed>
          <SexFrequency>1</SexFrequency>
        </statOffsets>
                <hediffGivers>
          <li Class="HediffGiver_Random">
            <hediff>tentaclegrowthS</hediff>
            <mtbDays>0.1</mtbDays>
            <partsToAffect>
              <li>Stomach</li>
            </partsToAffect>
            <countToAffect>1</countToAffect>
          </li>
          <li Class="HediffGiver_Random">
            <hediff>tentaclegrowthV</hediff>
            <mtbDays>0.1</mtbDays>
            <partsToAffect>
              <li>Genitals</li>
            </partsToAffect>
            <countToAffect>1</countToAffect>
          </li>
          <li Class="HediffGiver_Random">
            <hediff>tentaclegrowthA</hediff>
            <mtbDays>0.1</mtbDays>
            <partsToAffect>
              <li>Anus</li>
            </partsToAffect>
            <countToAffect>1</countToAffect>
          </li>
        </hediffGivers>
        <painOffset>0.1</painOffset>
      </li>
      <li>
        <label>incubating egg</label>
        <minSeverity>1.015</minSeverity>
      </li>
      <li>
        <label>hatched</label>
        <minSeverity>1.4</minSeverity>
            <hungerRateFactorOffset>0.1</hungerRateFactorOffset>
        <statOffsets>
          <Vulnerability>0.1</Vulnerability>
          <MoveSpeed>-0.01</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <label>larvae</label>
        <minSeverity>1.6</minSeverity>
        <restFallFactor>1.05</restFallFactor>
        <hungerRateFactorOffset>0.15</hungerRateFactorOffset>
        <statOffsets>
          <PsychicSensitivity>0.1</PsychicSensitivity>
          <Vulnerability>0.2</Vulnerability>
          <MoveSpeed>-0.02</MoveSpeed>
        </statOffsets>
                <hediffGivers>
          <li Class="HediffGiver_Random">
            <hediff>tentaclegrowthS</hediff>
            <mtbDays>4</mtbDays>
            <partsToAffect>
              <li>Stomach</li>
            </partsToAffect>
            <countToAffect>1</countToAffect>
          </li>
        </hediffGivers>
        <painOffset>0.02</painOffset>
      </li>
      <li>
        <label>nymph</label>
        <minSeverity>1.8</minSeverity>
        <restFallFactor>1.1</restFallFactor>
        <hungerRateFactorOffset>0.3</hungerRateFactorOffset>
        <statOffsets>
          <PsychicSensitivity>0.2</PsychicSensitivity>
          <Vulnerability>0.3</Vulnerability>
          <SexFrequency>0.5</SexFrequency>
          <MoveSpeed>-0.03</MoveSpeed>
        </statOffsets>
        <hediffGivers>
          <li Class="HediffGiver_Random">
            <hediff>tentaclegrowthS</hediff>
            <mtbDays>3</mtbDays>
            <partsToAffect>
              <li>Stomach</li>
            </partsToAffect>
            <countToAffect>1</countToAffect>
          </li>
          <li Class="HediffGiver_Random">
            <hediff>tentaclegrowthA</hediff>
            <mtbDays>2</mtbDays>
            <partsToAffect>
              <li>Anus</li>
            </partsToAffect>
            <countToAffect>1</countToAffect>
          </li>
        </hediffGivers>
        <painOffset>0.03</painOffset>
      </li>
      <li>
        <label>nearly grown</label>
        <minSeverity>2.5</minSeverity>
        <restFallFactor>1.2</restFallFactor>
        <hungerRateFactorOffset>0.5</hungerRateFactorOffset>
        <statOffsets>
          <PsychicSensitivity>0.3</PsychicSensitivity>
          <Vulnerability>0.5</Vulnerability>
          <SexFrequency>1</SexFrequency>
          <MoveSpeed>-0.05</MoveSpeed>
        </statOffsets>
                <hediffGivers>
          <li Class="HediffGiver_Random">
            <hediff>tentaclegrowthS</hediff>
            <mtbDays>0.1</mtbDays>
            <partsToAffect>
              <li>Stomach</li>
            </partsToAffect>
            <countToAffect>1</countToAffect>
          </li>
          <li Class="HediffGiver_Random">
            <hediff>tentaclegrowthA</hediff>
            <mtbDays>0.1</mtbDays>
            <partsToAffect>
              <li>Anus</li>
            </partsToAffect>
            <countToAffect>1</countToAffect>
          </li>
        </hediffGivers>
        <painOffset>0.1</painOffset>
      </li>
    </stages>
  </HediffDef>

  <HediffDef>
    <defName>tentaclegrowthA</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>stretched rectum</label>
    <labelNoun>rectum</labelNoun>
    <description>The Amor queen has grown and invaded this part of the body. The hosts rectum, and large intestine are being stretched to accommodate larger and more plentaful tentacle insertions. These processes are uncomfortable.</description>
    <defaultLabelColor>(255,20,147)</defaultLabelColor>
    <minSeverity>0.001</minSeverity>
    <initialSeverity>0.001</initialSeverity>
    <maxSeverity>1</maxSeverity>
    <stages>
      <li>
        <minSeverity>0.001</minSeverity>
        <label>still narrow</label>
        <painOffset>0.05</painOffset>
        <statOffsets>
          <MoveSpeed>-0.01</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.25</minSeverity>
        <label>expanding</label>
        <painOffset>0.2</painOffset>
        <statOffsets>
          <MoveSpeed>-0.02</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.5</minSeverity>
        <label>gaping:painfull</label>
        <painOffset>0.3</painOffset>
        <statOffsets>
          <MoveSpeed>-0.03</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.75</minSeverity>
        <label>gaping:acclimating</label>
        <painOffset>0.2</painOffset>
        <statOffsets>
          <MoveSpeed>-0.02</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>1</minSeverity>
        <label>gaping:accustomed</label>
        <painOffset>0.01</painOffset>
        <statOffsets>
          <TStorage>0.02</TStorage>
          <MoveSpeed>-0.01</MoveSpeed>
        </statOffsets>
      </li>
    </stages>
    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>0.25</severityPerDay>
      </li>
            <li Class="EBSGFramework.HediffCompProperties_GiveOtherHediffPeriodically">
                <hediff>AStorage</hediff>
                <severity>2</severity>
                <interval>5000</interval>
                <bodyParts>
                  <li>Anus</li>
                </bodyParts>
                <validSeverities>0.7~0.72</validSeverities>
            </li>
    </comps>
  </HediffDef>

<HediffDef>
    <defName>tentaclegrowthS</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>stretched stomach</label>
    <labelNoun>stomach</labelNoun>
    <description>The Amor queen has grown and invaded this part of the body. The hosts stomach, and throat are being stretched and cleared of acid to cultivate a better enviroment for tentacle offspring. These processes are uncomfortable.</description>
    <defaultLabelColor>(255,20,147)</defaultLabelColor>
    <minSeverity>0.001</minSeverity>
    <initialSeverity>0.001</initialSeverity>
    <maxSeverity>1</maxSeverity>
    <stages>
      <li>
        <minSeverity>0.001</minSeverity>
        <label>slightly streched</label>
        <painOffset>0.05</painOffset>
        <partEfficiencyOffset>-0.2</partEfficiencyOffset>
        <statOffsets>
        <VEF_FoodCapacityMultiplier>0.2</VEF_FoodCapacityMultiplier>
          <MoveSpeed>-0.01</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.25</minSeverity>
        <label>expanding</label>
        <painOffset>0.2</painOffset>
        <partEfficiencyOffset>-0.3</partEfficiencyOffset>
        <statOffsets>
        <VEF_FoodCapacityMultiplier>0.3</VEF_FoodCapacityMultiplier>
          <MoveSpeed>-0.02</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.5</minSeverity>
        <label>removing stomach acid</label>
        <painOffset>0.3</painOffset>
        <vomitMtbDays>0.08</vomitMtbDays>
        <partEfficiencyOffset>-0.8</partEfficiencyOffset>
        <statOffsets>
        <VEF_FoodCapacityMultiplier>0.5</VEF_FoodCapacityMultiplier>
          <MoveSpeed>-0.03</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.6</minSeverity>
        <label>spacious:acclimating</label>
        <painOffset>0.2</painOffset>
        <partEfficiencyOffset>-0.8</partEfficiencyOffset>
        <statOffsets>
        <VEF_FoodCapacityMultiplier>0.5</VEF_FoodCapacityMultiplier>
          <MoveSpeed>-0.03</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>1</minSeverity>
        <label>spacious:accustomed</label>
        <painOffset>0.01</painOffset>
        <partEfficiencyOffset>-0.7</partEfficiencyOffset>
        <statOffsets>
        <VEF_FoodCapacityMultiplier>0.5</VEF_FoodCapacityMultiplier>
          <MoveSpeed>-0.02</MoveSpeed>
          <TStorage>0.02</TStorage>
        </statOffsets>
      </li>
    </stages>
    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>0.25</severityPerDay>
      </li>
            <li Class="EBSGFramework.HediffCompProperties_GiveOtherHediffPeriodically">
                <hediff>SStorage</hediff>
                <severity>2</severity>
                <interval>5000</interval>
                <bodyParts>
                  <li>Stomach</li>
                </bodyParts>
                <validSeverities>0.7~0.72</validSeverities>
            </li>
    </comps>
  </HediffDef>


  <HediffDef>
    <defName>tentaclegrowthV</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>stretched womb</label>
    <labelNoun>womb</labelNoun>
    <description>The Amor queen has grown and invaded this part of the body. The hosts womb, and cervix are being stretched open to support longterm tentacle pregnacny. These processes are uncomfortable.</description>
