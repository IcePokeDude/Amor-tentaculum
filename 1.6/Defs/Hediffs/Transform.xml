<Defs>

    <HediffDef>
    <defName>transform</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>genetic body shaping</label>
    <labelNoun>genetic shaping</labelNoun>
    <description>The Amor queen has grown to adulthood and is now rewriting the hosts genetic code completely to suit the needs of her hive.</description>
    <defaultLabelColor>(255,20,147)</defaultLabelColor>
    <minSeverity>0.001</minSeverity>
    <initialSeverity>0.001</initialSeverity>
    <maxSeverity>1</maxSeverity>
    <stages>
      <li>
        <minSeverity>0.001</minSeverity>
        <painOffset>0.85</painOffset>
        <statOffsets>
          <MoveSpeed>-0.7</MoveSpeed>
        </statOffsets>
      </li>
    </stages>
    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>1.5</severityPerDay>
      </li>
      <li Class="EBSGFramework.HediffCompProperties_CreateOtherHediffs">
          <hediffSets>
                <li>
                  <hediffDef>VMtentacle</hediffDef>
                  <validSeverity>0.99</validSeverity>
                  <severityPerDay>0</severityPerDay>
                  <initialSeverity>1</initialSeverity>
                  <bodyPart>Genitals</bodyPart>
                </li>
                <li>
                  <hediffDef>AMtentacle</hediffDef>
                  <validSeverity>0.99</validSeverity>
                  <severityPerDay>0</severityPerDay>
                  <initialSeverity>1</initialSeverity>
                  <bodyPart>Anus</bodyPart>
                </li>
                <li>
                  <hediffDef>queenimplant</hediffDef>
                  <validSeverity>0~0.2</validSeverity>
                  <severityPerDay>-200</severityPerDay>
                  <initialSeverity>-1</initialSeverity>
                </li>
          </hediffSets>
      </li>
            <li Class="EBSGFramework.HediffCompProperties_AlterXenotype">
                <xenotypes>
                    <Thost>1</Thost>
                </xenotypes>
                <severities>1</severities>
                <setXenotype>False</setXenotype>
            </li>
        </comps>
  </HediffDef>

