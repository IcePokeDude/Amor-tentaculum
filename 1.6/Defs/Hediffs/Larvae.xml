<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <HediffDef ParentName="DiseaseBase">
    <defName>vagrancy</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>body vagrancy</label>
    <labelNoun>vagrancy</labelNoun>
    <description>This creature is suffering from not being within a host body.</description>
    <minSeverity>0</minSeverity>
    <initialSeverity>0.01</initialSeverity>
    <lethalSeverity>2</lethalSeverity>
    <maxSeverity>2</maxSeverity>
    <stages>
      <li>
        <minSeverity>1</minSeverity>
        <label>initial</label>
        <painOffset>0.20</painOffset>
        <statOffsets>
          <MoveSpeed>-0.3</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>1.3</minSeverity>
        <label>minor</label>
        <painOffset>0.40</painOffset>
        <statOffsets>
          <MoveSpeed>-0.6</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>1.6</minSeverity>
        <label>major</label>
        <painOffset>0.60</painOffset>
        <statOffsets>
          <MoveSpeed>-0.9</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>1.9</minSeverity>
        <label>extreme</label>
        <painOffset>0.90</painOffset>
        <statOffsets>
          <MoveSpeed>-1.2</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>2</minSeverity>
        <label>extreme</label>
        <painOffset>0.90</painOffset>
        <statOffsets>
          <MoveSpeed>-1.2</MoveSpeed>
        </statOffsets>
      </li>
    </stages>
    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>0.3</severityPerDay>
      </li>
    </comps>
  </HediffDef>

  <HediffDef>
    <defName>aggressive_nature</defName>
    <label>aggressive nature</label>
    <labelNoun>aggressive</labelNoun>
    <description>This creature is geneticly programed by the hive to be more aggressive in it's pursuit of intruders.</description>
    <stages>
      <li>
        <painOffset>-0.70</painOffset>
        <statOffsets>
          <MeleeCooldownFactor>-0.2</MeleeCooldownFactor>
        </statOffsets>
      </li>
    </stages>
  </HediffDef>

  <HediffDef>
    <defName>AStorage</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>Tentacles in Anus</label>
    <labelNoun>Amor Internum</labelNoun>
    <description>Amor Internum are living within the hosts ass. Tentacles in the anal cavity increase movement speed and can be birthed into bood nymphs.</description>
    <defaultLabelColor>(216,191,216)</defaultLabelColor>
    <minSeverity>1</minSeverity>
    <initialSeverity>1</initialSeverity>
    <maxSeverity>100</maxSeverity>
    <stages>
      <li>
        <minSeverity>1</minSeverity>
        <label>Few</label>
        <painOffset>-0.01</painOffset>
        <statOffsets>
          <MoveSpeed>0.01</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>10</minSeverity>
        <label>Many</label>
        <painOffset>-0.05</painOffset>
        <statOffsets>
          <MoveSpeed>0.05</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>40</minSeverity>
        <label>A lot</label>
        <painOffset>-0.1</painOffset>
        <statOffsets>
          <MoveSpeed>0.1</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>80</minSeverity>
        <label>A ton</label>
        <painOffset>-0.2</painOffset>
        <statOffsets>
          <MoveSpeed>0.2</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>100</minSeverity>
        <label>A silly amount</label>
        <painOffset>-0.3</painOffset>
        <statOffsets>
          <MoveSpeed>0.3</MoveSpeed>
        </statOffsets>
      </li>
      
    </stages>
    <comps>
      <li Class="EBSGFramework.HediffCompProperties_DisplaySeverity">
                <prependString>Number of occupants: </prependString>
                <appendString></appendString>
                <roundDigits>0</roundDigits>
      </li>
            <li Class="EBSGFramework.HediffCompProperties_GiveAbilityAtSeverity">
                <abilitiesAtSeverities>
                          <li>
                        <abilityDef>BirthA</abilityDef>
                        <validSeverity>2.0~999</validSeverity>
                    </li>
                </abilitiesAtSeverities>
            </li>
    </comps>
  </HediffDef>

    <HediffDef>
    <defName>VStorage</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>Tentacles in Womb</label>
    <labelNoun>Amor Internum</labelNoun>
    <description>Amor Internum are living within the hosts womb. Tentacles in the womb work to incubate huge Amor Matron Eggs, and increase psychic sensitivity.</description>
    <defaultLabelColor>(216,191,216)</defaultLabelColor>
    <minSeverity>0</minSeverity>
    <initialSeverity>1</initialSeverity>
    <maxSeverity>100</maxSeverity>
    <stages>
      <li>
        <minSeverity>1</minSeverity>
        <label>Few</label>
        <painOffset>0.01</painOffset>
        <statOffsets>
          <PsychicSensitivity>0.05</PsychicSensitivity>
        </statOffsets>
      </li>
      <li>
        <minSeverity>10</minSeverity>
        <label>Many</label>
        <painOffset>0.05</painOffset>
        <statOffsets>
          <PsychicSensitivity>0.1</PsychicSensitivity>
        </statOffsets>
      </li>
      <li>
        <minSeverity>40</minSeverity>
        <label>A lot</label>
        <painOffset>0.1</painOffset>
        <statOffsets>
          <PsychicSensitivity>0.25</PsychicSensitivity>
        </statOffsets>
      </li>
      <li>
        <minSeverity>80</minSeverity>
        <label>A ton</label>
        <painOffset>0.2</painOffset>
        <statOffsets>
          <PsychicSensitivity>0.5</PsychicSensitivity>
        </statOffsets>
      </li>
      <li>
        <minSeverity>100</minSeverity>
        <label>A silly amount</label>
        <painOffset>0.3</painOffset>
        <statOffsets>
          <PsychicSensitivity>1</PsychicSensitivity>
        </statOffsets>
      </li>
      
    </stages>
    <comps>
      <li Class="EBSGFramework.HediffCompProperties_DisplaySeverity">
                <prependString>Number of occupants: </prependString>
                <appendString></appendString>
                <roundDigits>0</roundDigits>
      </li>
      <li Class="EBSGFramework.HediffCompProperties_GiveOtherHediffPeriodically">
                <hediff>tentacleegg</hediff>
                <severity>0.005</severity>
                <interval>96000</interval>
                <bodyParts>
                 <li>Genitals</li>
                </bodyParts>
                <validSeverities>0.1</validSeverities>
                <multiplySeverityByParentSeverity>True</multiplySeverityByParentSeverity>
      </li>
    </comps>
  </HediffDef>

    <HediffDef>
    <defName>SStorage</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>Tentacles in Stomach</label>
    <labelNoun>Amor Internum</labelNoun>
    <description>Amor Internum are living within the hosts stomach. Tentacles in the stomach can be vomitted up as an attack, and increase host regeneration.</description>
    <defaultLabelColor>(216,191,216)</defaultLabelColor>
    <minSeverity>0</minSeverity>
    <initialSeverity>1</initialSeverity>
    <maxSeverity>100</maxSeverity>
    <stages>
      <li>
        <minSeverity>1</minSeverity>
        <label>Few</label>
        <painOffset>0.01</painOffset>
        <regeneration>5</regeneration>
      </li>
      <li>
        <minSeverity>10</minSeverity>
        <label>Many</label>
        <painOffset>0.05</painOffset>
        <regeneration>10</regeneration>
      </li>
      <li>
        <minSeverity>40</minSeverity>
        <label>A lot</label>
        <painOffset>0.1</painOffset>
        <regeneration>20</regeneration>
      </li>
      <li>
        <minSeverity>80</minSeverity>
        <label>A ton</label>
        <painOffset>0.2</painOffset>
        <regeneration>40</regeneration>
      </li>
      <li>
        <minSeverity>100</minSeverity>
        <label>A silly amount</label>
        <painOffset>0.3</painOffset>
        <regeneration>80</regeneration>
      </li>
      
    </stages>
    <comps>
      <li Class="EBSGFramework.HediffCompProperties_DisplaySeverity">
                <prependString>Number of occupants: </prependString>
                <appendString></appendString>
                <roundDigits>0</roundDigits>
      </li>
      <li Class="EBSGFramework.HediffCompProperties_GiveAbilityAtSeverity">
                <abilitiesAtSeverities>
                    <li>
                        <abilityDef>AttackTent</abilityDef>
                        <validSeverity>2.0~999</validSeverity>
                    </li>
                    <li>
                        <abilityDef>TCumSpray</abilityDef>
                        <validSeverity>2.0~999</validSeverity>
                    </li>
                </abilitiesAtSeverities>
       </li>
      </comps>
  </HediffDef>


</Defs>