			<color>(203,125,227)</color>
		</graphicData>
		<statBases>
			<MarketValue>1.0</MarketValue>
		</statBases>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>2</glowRadius>
				<glowColor>(203,125,227)</glowColor>
			</li>
		</comps>
	</ThingDef>

	<HediffDef>
    <defName>TEggAss</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>incubating egg</label>
    <labelNoun>egg</labelNoun>
    <description>Internal tentacles are working to produce and incubate a warrior egg in the hosts anus.</description>
    <defaultLabelColor>(203,120,227)</defaultLabelColor>
    <minSeverity>0.001</minSeverity>
    <initialSeverity>0.001</initialSeverity>
    <maxSeverity>1</maxSeverity>
    <stages>
      <li>
        <minSeverity>0.001</minSeverity>
        <label>pea sized</label>
        <painOffset>0.05</painOffset>
        <partEfficiencyOffset>-0.2</partEfficiencyOffset>
        <statOffsets>
          <MoveSpeed>-0.03</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.25</minSeverity>
        <label>grape sized</label>
        <painOffset>0.05</painOffset>
        <statOffsets>
          <MoveSpeed>-0.05</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.5</minSeverity>
        <label>peach sized</label>
        <painOffset>0.1</painOffset>
        <statOffsets>
          <MoveSpeed>-0.08</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.75</minSeverity>
        <label>apple sized</label>
        <painOffset>0.2</painOffset>
        <statOffsets>
          <MoveSpeed>-0.1</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.95</minSeverity>
        <label>laying egg</label>
        <painOffset>0.8</painOffset>
        <statOffsets>
          <MoveSpeed>-3</MoveSpeed>
        </statOffsets>
      </li>
    </stages>
    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>0.4</severityPerDay>
      </li>
            <li Class="EBSGFramework.HediffCompProperties_RemoveAtSeverities">
                <severity>1</severity>
            </li>
            <li Class="EBSGFramework.HediffCompProperties_CreateItems">
                <onDeathOrRemovalThings>
                    <li>
                        <li><thing>MedTEgg</thing></li>
                    </li>
                </onDeathOrRemovalThings>
            </li>
    </comps>
  </HediffDef>

	<HediffDef>
    <defName>TEggStomach</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>incubating eggs</label>
    <labelNoun>eggs</labelNoun>
    <description>Internal tentacles are working to produce and incubate a clutch of drone eggs in the hosts stomach.</description>
    <defaultLabelColor>(203,120,227)</defaultLabelColor>
    <minSeverity>0.001</minSeverity>
    <initialSeverity>0.001</initialSeverity>
    <maxSeverity>1</maxSeverity>
    <stages>
      <li>
        <minSeverity>0.001</minSeverity>
        <label>forming</label>
        <painOffset>0.05</painOffset>
        <partEfficiencyOffset>-0.2</partEfficiencyOffset>
        <statOffsets>
          <MoveSpeed>-0.03</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.25</minSeverity>
        <label>incubating</label>
        <painOffset>0.05</painOffset>
        <statOffsets>
          <MoveSpeed>-0.05</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.5</minSeverity>
        <label>incubating</label>
        <painOffset>0.1</painOffset>
        <statOffsets>
          <MoveSpeed>-0.08</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.75</minSeverity>
        <label>incubating</label>
        <painOffset>0.2</painOffset>
        <statOffsets>
          <MoveSpeed>-0.1</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.95</minSeverity>
        <label>vomiting eggs</label>
        <painOffset>0.8</painOffset>
		<vomitMtbDays>0.01</vomitMtbDays>
        <statOffsets>
          <MoveSpeed>-3</MoveSpeed>
        </statOffsets>
      </li>
    </stages>
    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>0.95</severityPerDay>
      </li>
            <li Class="EBSGFramework.HediffCompProperties_RemoveAtSeverities">
                <severity>1</severity>
            </li>
            <li Class="EBSGFramework.HediffCompProperties_CreateItems">
                <intervalTicks>60900</intervalTicks>
                <intervalThings>
                    <li>
                        <li>
                            <thing>DroneTEgg</thing>
                            <count>4</count>
                            <weight>1</weight>
                        </li>
                        <li>
                            <thing>DroneTEgg</thing>
                            <count>3</count>
                            <weight>2</weight>
                        </li>
                        <li>
                            <thing>DroneTEgg</thing>
                            <count>2</count>
                            <weight>2</weight>
                        </li>
                    </li>
                </intervalThings>
                <onDeathOrRemovalThings>
                    <li>
                        <li><thing>DroneTEgg</thing></li>
                    </li>
                </onDeathOrRemovalThings>
            </li>
    </comps>
  </HediffDef>

    <HediffDef>
    <hediffClass>HediffWithComps</hediffClass>
    <defName>tentacleegg</defName>
    <label>Amor Egg</label>
    <labelNoun>an amor egg</labelNoun>
    <description>A huge tentacle egg has been produced by amor queen and swollen inside the hosts body.</description>
    <renderNodeProperties>
      <li Class="PawnRenderNodeProperties_Eye">
        <workerClass>PawnRenderNodeWorker_HediffEye</workerClass>
        <texPath>Things/Pawns/Parts/Addons/Belly</texPath>
        <parentTagDef>Body</parentTagDef>
        <colorType>Skin</colorType>
        <rotDrawMode>Fresh, Rotting</rotDrawMode>
        <useSkinShader>true</useSkinShader>
        <useRottenColor>true</useRottenColor>
        <drawSize>1.2</drawSize>
        <drawData>
          <defaultData>
            <layer>39</layer>
            <offset>(0, 0, -0.1)</offset>
          </defaultData>
          <dataNorth>
            <layer>0</layer>
            <offset>(0, 0, 0)</offset>
          </dataNorth>
          <dataEast>
            <flip>false</flip>
            <offset>(0, 0, 0)</offset>
            <layer>-10</layer>
          </dataEast>
          <dataSouth>
            <offset>(0, 0, -0.1)</offset>
            <layer>20</layer>
          </dataSouth>
          <dataWest>
            <layer>-10</layer>
          </dataWest>
        </drawData>
      </li>
    </renderNodeProperties>
    <stages>
      <li>
        <minSeverity>0.001</minSeverity>
        <label>tiny</label>
        <statOffsets>
          <MoveSpeed>-0.01</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.1</minSeverity>
        <label>small</label>
        <painOffset>0.1</painOffset>
        <statOffsets>
          <MoveSpeed>-0.05</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.3</minSeverity>
        <label>large</label>
        <painOffset>0.1</painOffset>
        <statOffsets>
          <MoveSpeed>-0.05</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.5</minSeverity>
        <label>huge</label>
        <painOffset>0.2</painOffset>
        <statOffsets>
          <MoveSpeed>-0.2</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.7</minSeverity>
        <label>watermelon sized</label>
        <partEfficiencyOffset>-0.20</partEfficiencyOffset>
        <painOffset>0.40</painOffset>
        <statOffsets>
          <MoveSpeed>-0.4</MoveSpeed>
        </statOffsets>
      </li>
    </stages>
        <comps>
      <li Class="EBSGFramework.HediffCompProperties_GiveAbilityAtSeverity">
                <abilitiesAtSeverities>
                          <li>
                        <abilityDef>BirthV</abilityDef>
                        <validSeverity>0.98~999</validSeverity>
                    </li>
                </abilitiesAtSeverities>
       </li>
      </comps>
  </HediffDef>

      <HediffDef>
    <defName>EggLabor</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>Laying Egg</label>
    <labelNoun>laying egg</labelNoun>
    <description>Due too the size of the egg the labor processes is quite intensive.</description>
    <defaultLabelColor>(255,20,147)</defaultLabelColor>
    <minSeverity>0</minSeverity>
    <initialSeverity>1</initialSeverity>
    <maxSeverity>1</maxSeverity>
    <stages>
           <li>
        <minSeverity>0.1</minSeverity>
        <label>recovering</label>
        <painOffset>0.7</painOffset>
        <statOffsets>
          <MoveSpeed>-0.9</MoveSpeed>
        </statOffsets>
      </li>
       <li>
        <minSeverity>0.15</minSeverity>
        <label>egg crowning</label>
        <painOffset>0.95</painOffset>
        <statOffsets>
          <MoveSpeed>-0.9</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.4</minSeverity>
        <label>pushing</label>
        <painOffset>0.9</painOffset>
        <statOffsets>
          <MoveSpeed>-1</MoveSpeed>
        </statOffsets>
      </li>
       <li>
        <minSeverity>0.6</minSeverity>
        <label>resting</label>
        <painOffset>0.3</painOffset>
        <statOffsets>
          <MoveSpeed>-0.9</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.8</minSeverity>
        <label>pushing</label>
        <painOffset>0.9</painOffset>
        <statOffsets>
          <MoveSpeed>-1</MoveSpeed>
        </statOffsets>
      </li>
    </stages>
    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>-2</severityPerDay>
      </li>
        <li Class="EBSGFramework.HediffCompProperties_CreateItems">
                <intervalTicks>25400</intervalTicks>
                <intervalThings>
                    <li>
                        <li>
                            <thing>BigTEgg</thing>
                            <count>1</count>
                            <weight>1</weight>
                        </li>
                    </li>
                </intervalThings>
            </li>
        </comps>
  </HediffDef>

</Defs>