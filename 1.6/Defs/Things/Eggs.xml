<?xml version="1.0" encoding="utf-8"?>
<Defs>
 <ThingDef ParentName="EggFertBase">
    <defName>BigTEgg</defName>
    <label>Huge Tentacle Egg</label>
    <description>A huge tentacle egg produced in the womb.</description>
    <possessionCount>1</possessionCount>
    <graphicData>
      <texPath>Things/Items/HugeEgg</texPath>
    </graphicData>
    <statBases>
      <MarketValue>50.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>10</hatcherDaystoHatch>
        <hatcherPawn>TMatron</hatcherPawn>
      </li>
    </comps>
  </ThingDef>

 <ThingDef ParentName="EggFertBase">
    <defName>MedTEgg</defName>
    <label>Large Tentacle Egg</label>
    <description>A large tentacle egg produced by a tentacle host.</description>
    <possessionCount>1</possessionCount>
    <graphicData>
      <texPath>Things/Items/MedEgg</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>50.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>3</hatcherDaystoHatch>
        <hatcherPawn>TLarvaMat</hatcherPawn>
      </li>
    </comps>
  </ThingDef>

   <ThingDef ParentName="EggFertBase">
    <defName>DroneTEgg</defName>
    <label>Drone Egg</label>
    <description>A small drone egg produced by a tentacle host.</description>
    <possessionCount>1</possessionCount>
    <graphicData>
      <texPath>Things/Item/Resource/AnimalProductRaw/EggBirdSmall</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
      <color>(203,120,227)</color>
    </graphicData>
    <statBases>
      <MarketValue>50.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>1</hatcherDaystoHatch>
        <hatcherPawn>TDrone</hatcherPawn>
      </li>
    </comps>
  </ThingDef>

	<rjw.HediffDef_InsectEgg Name="RJW_ImplantEgg" Abstract="True">
