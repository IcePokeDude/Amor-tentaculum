          <MoveSpeed>-0.03</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.5</minSeverity>
        <label>first trimester</label>
        <painOffset>0.05</painOffset>
        <statOffsets>
          <MoveSpeed>-0.05</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>2.5</minSeverity>
        <label>second trimester</label>
        <painOffset>0.1</painOffset>
        <statOffsets>
          <MoveSpeed>-0.08</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>4</minSeverity>
        <label>third trimester</label>
        <painOffset>0.2</painOffset>
        <statOffsets>
          <MoveSpeed>-0.1</MoveSpeed>
        </statOffsets>
      </li>
      <li>
        <minSeverity>4.95</minSeverity>
        <label>vomiting egg</label>
        <painOffset>0.8</painOffset>
		<vomitMtbDays>0.01</vomitMtbDays>
        <statOffsets>
          <MoveSpeed>-3</MoveSpeed>
        </statOffsets>
      </li>
    </stages>
    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>0.001</severityPerDay>
      </li>
            <li Class="EBSGFramework.HediffCompProperties_RemoveAtSeverities">
                <severity>5</severity>
            </li>
            <li Class="EBSGFramework.HediffCompProperties_CreateItems">
                <onDeathOrRemovalThings>
                    <li>
                        <li><thing>DroneTEgg</thing></li>
                    </li>
                </onDeathOrRemovalThings>
            </li>
            <li Class="EBSGFramework.HediffCompProperties_GiveOtherHediffPeriodically">
                <hediff>Vomitegg</hediff>
                <severity>0.01</severity>
                <validSeverities>4.95</validSeverities>
                <multiplySeverityByParentSeverity>false</multiplySeverityByParentSeverity>
                <bodyParts>
                  <li>Jaw</li>
                </bodyParts>
            </li>
    </comps>
  </HediffDef>

