    <AbilityDef>
        <defName>BirthAM</defName>
        <label>incubate warrior egg</label>
        <description>Reform an internal tentacle from the anus into a malum eel, then give birth too it. Using this while an eel is already incubating increase the incubation speed.</description>
        <iconPath>Abilities/OviAss</iconPath>
        <category>Sanguophage</category>
        <targetRequired>False</targetRequired>
        <cooldownTicksRange>60</cooldownTicksRange>
        <hostile>false</hostile>
        <casterMustBeCapableOfViolence>false</casterMustBeCapableOfViolence>
        <verbProperties>
            <verbClass>Verb_CastAbility</verbClass>
            <range>10</range>
            <warmupTime>2.5</warmupTime>
            <soundCast>PiercingSpine_Launch</soundCast>
            <requireLineOfSight>False</requireLineOfSight>
            <targetParams>
                <canTargetSelf>true</canTargetSelf>
                <canTargetLocations>true</canTargetLocations>
            </targetParams>
        </verbProperties>
        <comps>
            <li Class="EBSGFramework.CompProperties_AbilityAbilityValidator">
            <casterHasAnyOfHediffs>
                <li>AStorage</li>
            </casterHasAnyOfHediffs>
            </li>
            <li Class="EBSGFramework.CompProperties_AbilityResourceCost">
                <resourceCost>0.01</resourceCost>
                <mainResourceGene>tentaclebabyneedM</mainResourceGene>
            </li>
            <li Class="EBSGFramework.CompProperties_AbilityGiveMultipleHediffs">
                <hediffsToGive>
                    <li>
                        <hediffDef>AStorage</hediffDef>
                        <onlyApplyToSelf>true</onlyApplyToSelf>
                        <replaceExisting>false</replaceExisting>
                        <bodyParts>
                            <li>Anus</li>
                        </bodyParts>
                        <severity>-1</severity>
                    </li>
            </hediffsToGive>
            </li>
            <li Class="EBSGFramework.CompProperties_AbilitySpawnPawnKind">
                <pawnKind>MEel</pawnKind>
                <count>1</count>
                <mentalStateOnSpawn>EBSG_AggressiveSummon</mentalStateOnSpawn>
            </li>
        </comps>
    </AbilityDef>

    <AbilityDef>
        <defName>AttackTent</defName>
        <label>release attack tentacle</label>
        <description>Release a hostile tentacle that will seek out victims to attach too and incapacitate.</description>
        <iconPath>Abilities/OviMouth</iconPath>
        <category>Sanguophage</category>
        <targetRequired>True</targetRequired>
        <cooldownTicksRange>460</cooldownTicksRange>
        <hostile>false</hostile>
        <casterMustBeCapableOfViolence>false</casterMustBeCapableOfViolence>
        <verbProperties>
            <verbClass>Verb_CastAbility</verbClass>
            <range>2</range>
            <warmupTime>3.5</warmupTime>
            <soundCast>PiercingSpine_Launch</soundCast>
            <requireLineOfSight>False</requireLineOfSight>
            <targetParams>
                <canTargetLocations>true</canTargetLocations>
            </targetParams>
        </verbProperties>
        <comps>
                    <li Class="EBSGFramework.CompProperties_AbilityAbilityValidator">
            <casterHasAnyOfHediffs>
                <li>SStorage</li>
            </casterHasAnyOfHediffs>
            </li>
            <li Class="EBSGFramework.CompProperties_AbilityResourceCost">
                <resourceCost>0.01</resourceCost>
                <mainResourceGene>tentaclebabyneed</mainResourceGene>
            </li>
            <li Class="EBSGFramework.CompProperties_AbilityGiveMultipleHediffs">
                <hediffsToGive>
                    <li>
                        <hediffDef>SStorage</hediffDef>
                        <onlyApplyToSelf>true</onlyApplyToSelf>
                        <replaceExisting>false</replaceExisting>
                        <severity>-1</severity>
                    </li>
                    <li>
                        <hediffDef>VMAttackTent</hediffDef>
                        <onlyApplyToSelf>true</onlyApplyToSelf>
                        <replaceExisting>false</replaceExisting>
                        <bodyParts>
                            <li>Stomach</li>
                        </bodyParts>
                        <severity>1</severity>
                    </li>
            </hediffsToGive>
            </li>
            <li Class="EBSGFramework.CompProperties_AbilitySpawnPawnKind">
                <pawnKind>TGrabber</pawnKind>
                <count>3</count>
                <mentalStateOnSpawn>EBSG_AggressiveSummon</mentalStateOnSpawn>
                <hediffOnPawns>BodyDependance</hediffOnPawns>
            </li>
        </comps>
    </AbilityDef>
</Defs>