      <GeneDef ParentName="GeneGenitalTypeBase">
    <defName>rjw_genes_tentinfest_genitalia</defName>
    <label>tentacle inhabited genitalia</label>
    <description>Carriers of this gene have genitalia permantly inhabited by tentacles.</description>
    <iconPath>Genes/Tentacle_Vagina</iconPath>
    <modExtensions>
      <li Class="RJW_Genes.GenitaliaTypeExtension">
        <penis>Penis</penis>
        <vagina>TVagina</vagina>
        <anus>TAnus</anus>
        <breasts>TBreasts</breasts>
      </li>
    </modExtensions>
    <displayOrderInCategory>802</displayOrderInCategory>
  </GeneDef>

</Defs>