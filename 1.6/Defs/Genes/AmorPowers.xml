<Defs>

    <AbilityDef ParentName="AbilityTouchBase">
        <defName>UnbirthAi</defName>
        <label>crawl inside anus</label>
        <description>Commands the Internum to crawl inside a incubator's anus to increase drone production.</description>
        <iconPath>Abilities/ButtVore</iconPath>
        <displayGizmoWhileUndrafted>true</displayGizmoWhileUndrafted>
        <disableGizmoWhileUndrafted>false</disableGizmoWhileUndrafted>
        <cooldownTicksRange>5</cooldownTicksRange>
        <aiCanUse>true</aiCanUse>
        <hostile>false</hostile>
        <casterMustBeCapableOfViolence>false</casterMustBeCapableOfViolence>
        <verbProperties>
         <verbClass>Verb_CastAbilityTouch</verbClass>
         <drawAimPie>false</drawAimPie>
         <range>1</range>
         <warmupTime>2</warmupTime>
         <soundCast>AcidSpray_Resolve</soundCast>
            <targetParams>
            <canTargetPawns>true</canTargetPawns>
            </targetParams>
        </verbProperties>
        <comps>
			<li Class="EBSGFramework.CompProperties_AbilityGiveResource">
                <amount>0.01</amount>
                <mainResourceGene>tentaclebabyneed</mainResourceGene>
            </li>
            <li Class="EBSGFramework.CompProperties_AbilityAbilityValidator">
            <targetHasAnyOfHediffs>
                <li>tentaclegrowthA</li>
            </targetHasAnyOfHediffs>
            <casterHasNoneOfHediffs>
                <li>FullofTentacles</li>
            </casterHasNoneOfHediffs>
            </li>
            <li Class="EBSGFramework.CompProperties_AbilityGiveMultipleHediffs">
                <hediffsToGive>
				<li>
                    <hediffDef>AStorage</hediffDef>
                    <onlyApplyToSelf>false</onlyApplyToSelf>
                    <severity>1</severity>
                    <bodyParts>
                        <li>Anus</li>
                    </bodyParts>
                    </li>
                    <li>
                    <hediffDef>BodyCleaner</hediffDef>
                        <onlyApplyToSelf>true</onlyApplyToSelf>
                        <replaceExisting>false</replaceExisting>
                    <bodyParts>
                        <li>Brain</li>
                    </bodyParts>
                    </li>
                    <li>
                    <hediffDef>tentaclegrowthA</hediffDef>
                        <replaceExisting>false</replaceExisting>
                    <bodyParts>
                        <li>Anus</li>
                    </bodyParts>
                    </li>
                </hediffsToGive>
            </li>
            <li Class="CompProperties_AbilityFleckOnTarget">
        <fleckDef>Heart</fleckDef>
            </li>
            </comps>
</AbilityDef>

