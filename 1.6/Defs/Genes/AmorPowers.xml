  <HediffDef>
    <defName>maketentbeer</defName>
    <hediffClass>HediffWithComps</hediffClass>
    <label>producing Amornuum</label>
    <labelNoun>producing</labelNoun>
    <description>This creature is producing Amornuum in it's many dick tentacles. Amornuum is an alcohol like substance that is also a minor aphrodisiac. Produces once a day.</description>
    <stages>
      <li>
        <painOffset>-0.40</painOffset>
        <statOffsets>
          <MeleeCooldownFactor>0.05</MeleeCooldownFactor>
          <MoveSpeed>-0.1</MoveSpeed>
        </statOffsets>
      </li>
    </stages>
        <comps>
            <li Class="EBSGFramework.HediffCompProperties_CreateItems">
                <intervalTicks>60000</intervalTicks>
                <intervalThings>
                    <li>
                        <li>
                            <thing>TentBeer</thing>
                            <count>5</count>
                        </li>
                        <li>
                            <thing>TentBeer</thing>
                            <count>8</count>
                        </li>
                        <li>
                            <thing>TentBeer</thing>
                            <count>10</count>
                        </li>
                        <li>
                            <thing>TentBeer</thing>
                            <count>12</count>
                        </li>
                    </li>
                </intervalThings>
            </li>
        </comps>
  </HediffDef>

